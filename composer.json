{
  "name": "tribes/boilerplate",
  "description": "Boilerplate",
  "type": "wordpress-theme",
  "minimum-stability": "stable",
  "authors": [],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "require": {
    "vlucas/phpdotenv": "^5.4",
    "timber/timber": "^1.20"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
