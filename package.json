      "base": "dist/",
      "css": "dist/css/",
      "js": "dist/js/",
      "images": "dist/images/",
      "favicon": "dist/images/favicon/"
    },
    "assets": {
      "base": "src/",
      "scss": "src/scss/",
      "js": "src/js/",
      "images": "src/images/",
      "favicon": "src/images/favicon/",
      "fonts": "src/fonts/"
    }
  },
  "files": {
    "tailwind": "./tailwind.config.js",
    "dist": {
      "css": "app.css",
      "js": "app.js"
    },
    "assets": {
      "scss": "app.scss",
      "js": "index.js"
    }
  },
  "critical": {
    "dest": "_criticalCss/",
    "elements": []
  },
  "purgeCss": {
    "whitelist": [],
