{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "license": "ISC",
  "type": "module",
  "env": {
    "local": ""
  },
  "paths": {
    "dependencies": "node_modules/",
    "public": "",
    "templates": "templates/",
